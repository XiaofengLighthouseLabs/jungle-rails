<h1>Log In</h1>

<%= form_tag '/login' do %>
  <div class="form-group">
    <div>
      Email:
    </div>
    <div>
      <%= text_field_tag :email %>
    </div>
  </div>
  <div class="form-group">
    <div>
      Password:
    </div>
    <div>
      <%= password_field_tag :password %>
    </div>
  </div>
  <div class="form-group">
      <%= submit_tag "Submit" %>
  </div>

<% end %>